{
  "name": "pantry.api",
  "version": "0.1.0",
  "description": "Pantry API",
  "author": "danilo4lves",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=production nest start",
    "start:dev": "cross-env NODE_ENV=development ts-node-dev --inspect --clear --no-notify -r tsconfig-paths/register src/main.ts",
    "start:homolog": "NODE_ENV=homolog node dist/main",
    "start:production": "NODE_ENV=production node dist/main",
    "typeorm": "cross-env NODE_ENV=development ts-node-dev --no-notify --clear -r tsconfig-paths/register node_modules/typeorm/cli",
    "typeorm:homolog": "cross-env NODE_ENV=homolog ts-node-dev --no-notify --clear -r tsconfig-paths/register node_modules/typeorm/cli",
    "typeorm:prod": "cross-env NODE_ENV=production ts-node-dev --no-notify --clear -r tsconfig-paths/register node_modules/typeorm/cli",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:cov": "jest --coverage --detectOpenHandles",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest.config-e2e.js --runInBand",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.1",
    "@nestjs/common": "^8.0.6",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.6",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/terminus": "^8.0.3",
    "@nestjs/throttler": "^2.0.0",
    "@nestjs/typeorm": "^8.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "crypto-js": "^4.1.1",
    "form-data": "^4.0.0",
    "helmet": "^4.6.0",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "node-loggly-bulk": "^3.0.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "typeorm": "^0.2.37",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.6",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.175",
    "@types/node": "^16.7.1",
    "@types/node-loggly-bulk": "^2.2.1",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^5.5.3",
    "fishery": "^1.4.0",
    "husky": "^7.0.2",
    "jest": "^27.0.6",
    "lint-staged": ">=11",
    "prettier": "^2.2.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.3.5"
  },
  "volta": {
    "node": "14.17.5",
    "yarn": "1.22.11"
  }
}
